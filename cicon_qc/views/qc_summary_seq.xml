<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for QC -->


        <record id="sequence_qc_summary" model="ir.sequence">
            <field name="name">QC Summary</field>
            <field name="code">cic.qc.summary.seq</field>
            <field name="prefix"></field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="True"/>
            <!--<field name="implementation">no_gap</field>-->
        </record>



        <record id="sequence_qc_observation_report" model="ir.sequence">
            <field name="name">QC Observation</field>
            <field name="code">cic.qc.observation.seq</field>
            <field name="prefix">QOR/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="True"/>
            <!--<field name="implementation">no_gap</field>-->
        </record>


          <record id="sequence_qc_check" model="ir.sequence">
            <field name="name">QC Check</field>
            <field name="code">cic.qc.check.seq</field>
            <field name="prefix">QC/CH/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="True"/>
            <!--<field name="implementation">no_gap</field>-->
        </record>

    </data>
</odoo>
